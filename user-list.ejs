    <div class="table-data">
<h2>customers cars</h2>
    <table style="width: 200px; border: 1px solid black">
        <tr>
            <th>ID</th>
            <th>Car Name</th>
            <th>Company</th>
            <th>price</th>
            <th>Email</th>
            <th>Phone</th>
    <!--        <th>pass</th>   
            <th>Edit</th>
            <th>Delete</th> !-->
        </tr>
        
        <%
        if(userData.length!=0){
        var i=1;
        userData.forEach(function(data){
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.carname %></td>
            <td><%=data.company %></td>
            <td><%=data.price %></td>
            <td><%=data.email %></td>
            <td><%=data.phone %></td>
         
           
         <!--   <td>=data.pass %></td>
            <td><a href="/users/edit/<//=data.id%>">Edit</a></td>
            <td><a href="/users/delete/<//=data.id%>">Delete</a></td> !-->
        </tr>
         <tr colspan="20">
             <td colspan="40"><img src ="<%=data.pic %>"/></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
